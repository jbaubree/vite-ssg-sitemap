    "fast-glob": "^3.2.11",
    "pkgroll": "^1.0.3",
    "sitemap": "^7.1.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.9.0",
    "vue-tsc": "^0.33.9",
    "xml-formatter": "^2.6.1"
