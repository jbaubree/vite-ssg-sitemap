    "release": "npx bumpp --commit --tag --push && npm publish",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.26.1",
    "@types/node": "18.6.4",
    "eslint": "8.22.0",
    "pkgroll": "1.4.0",
    "sitemap-ts": "0.2.7",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vue-tsc": "0.40.1"
  }
}
