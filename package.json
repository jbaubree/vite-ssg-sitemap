{
  "name": "vite-ssg-sitemap",
  "version": "0.3.2",
  "description": "sitemap generator working with vite-ssg",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "dev": "npm run build -- --watch",
    "example:build": "npm -C example run build",
