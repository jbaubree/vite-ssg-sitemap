    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.1",
    "@types/node": "17.0.25",
    "eslint": "8.18.0",
    "pkgroll": "1.3.1",
    "sitemap-ts": "0.1.6",
    "typescript": "4.7.4",
    "vite": "2.9.12",
    "vue-tsc": "0.38.1"
  }
}
